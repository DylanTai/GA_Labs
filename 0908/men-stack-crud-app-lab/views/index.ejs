<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Plants</title>
  </head>
  <body>
    <h1>All Plants</h1>
    <a href="/plants/new">New Plant</a>
    <ul>
      <% plants.forEach(p => { %>
      <li>
        <a href="/plants/<%= p._id %>"><%= p.name %></a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
